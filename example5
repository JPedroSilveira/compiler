int z;
int reflect(int a, int b, int c, int d, int e, int f) {
    int z;
    z = a + b + c + d + e + f;
    return z;
}
int main(int a) {
    int result;
    result = 0;
    z = 4;
    while (z != 0) {
        result = result + reflect(1, 2, 3, 4, 5, 6);
        z = z - 1;
    };
    return result;
}